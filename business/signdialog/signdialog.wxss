}

.getCoupleSuccess image {
  width: 580rpx;
  height: 800rpx;
}

.getCoupleSuccess .form {
  position: absolute;
  bottom: 140rpx;
  width: 380rpx;
  height: 230rpx;
  left:102rpx;
}

