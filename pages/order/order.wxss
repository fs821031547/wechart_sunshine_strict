page{padding-bottom: 30rpx;}
.win200{width:90rpx;}
.OrderBox{margin: 30rpx 30rpx 0 30rpx; background-color: #fff;border-radius:16rpx; padding: 0 40rpx;box-shadow:0rpx 0rpx 14rpx #eee;position: relative;overflow: hidden;}
.OrderBox .title{display: flex;align-items: center;padding: 30rpx 0 20rpx;}
.OrderBox .title .tit{font-size: 38rpx;flex: 1;}
.OrderBox .title .dat{font-size: 26rpx;color: #999;}

.OrderBox .gap{ height:14rpx; border-bottom: 1rpx dashed #ddd; position: relative; margin-bottom: 14rpx;}
.OrderBox .gap:after,.OrderBox .gap:before{ content: ""; width:28rpx; height:28rpx; background-color: #f5f5f5; display: block; position: absolute;border-radius: 100%;}
.OrderBox .gap:after{left: -58rpx;}
.OrderBox .gap:before{right: -58rpx;}

.OrderBox .txtle{padding:15rpx 0; font-size: 30rpx;}
.OrderBox .txtle .txt{margin-bottom:8rpx;}
.OrderBox .txtle .c999{color: #999;}
.OrderBox .txtle .mr10{margin-right: 10rpx;}
.OrderBox .flex{display: flex;align-items: center;}
.OrderBox .flex .Model{margin-right:10rpx;background-color: #008cf8;font-size: 26rpx; height:34rpx; line-height: 32rpx; color: #fff;  padding: 0 10rpx;border-radius: 6rpx;}
.OrderBox .flex .frs{flex: 1;}

.OrderBox .butbox{display: flex;align-items: center;padding:25rpx 0; border-top: 1rpx dashed #ddd; position:relative;}
.OrderBox .butbox .Prepaid{font-size: 30rpx;}
.OrderBox .butbox .money{color: #e62424;font-size: 34rpx;}
.buttonfrs{position: absolute;right:0;display: flex;align-items: center;}
button{margin:0 0 0 15rpx;}
button::after {
  border: none;
}
.buttona {background: linear-gradient(#ff7200, #fa0808) no-repeat; color: #fff;font-size: 30rpx;padding: 0 15rpx; line-height: 60rpx; height: 60rpx;}
.buttonb {background-color: #e6e6e6;font-size: 30rpx;padding: 0 15rpx; line-height: 60rpx; height: 60rpx;}

.podT{position: absolute;width:140rpx; height:141rpx;left: 50%; margin-left: -70rpx;top:-40rpx;}

.Invalid .title,.Invalid .txtle {color: #999;}
.Invalid .flex .Model{background-color: #ddd;}