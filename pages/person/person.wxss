}

.persontxt {
  background-color: #fff;
  margin: 0 30rpx;
  border-radius: 16rpx;
  padding: 10rpx 30rpx;
  box-shadow: 0rpx 8rpx 8rpx #eee;
}

.persontxt .time {
  display: flex;
  align-items: center;
}

.persontxt .time .teltxt {
  font-size: 42rpx;
  font-weight: 700;
  background-image: -webkit-linear-gradient(bottom, #fa0808, #ff7200);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.persontxt .small-wrp {
  margin-right: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.persontxt .small-wrp .imga {
