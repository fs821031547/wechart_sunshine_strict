/* weui compontent */

.weui-cells:after, .weui-cells:before {
  position: absolute;
  left: 0;
  right: 0;
  height: 1px;
  color: rgba(0, 0, 0, 0.1);
}

.weui-cells {
  position: relative;
  background-color: #fff;
  line-height: 1.41176471;
  padding: 20rpx 0;
}
.weui-cells-bottom{
   margin-bottom:16rpx;
}

.r-blank {
  width: 100%;
  height: 70rpx;
  background: #fff;
}

.weui-cells:before {
  top: 0;
  /* border-top: 1rpx solid rgba(0,0,0,.1); */
}

.weui-cell_access {
  color: inherit;
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}

.weui-cell {
  /* padding: 13px 32px; */
  padding: 13px 60rpx;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}

