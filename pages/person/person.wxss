.personalbg button {
  background-color: transparent;
  padding: 0;
  z-index: 101;
  line-height: normal;
  color: #666;
}

.personalbg button::after {
  border: none;
}

.personalbg .Headicon {
  width: 138rpx;
  height: 138rpx;
  margin: 0 auto;
  border: 9rpx solid #fff;
  border-radius: 100%;
  background-color: #fff;
}

.personalbg .Headicon image {
  border-radius: 100%;
  vertical-align: middle;
}

.personalbg .Headpu {
  text-align: center;
