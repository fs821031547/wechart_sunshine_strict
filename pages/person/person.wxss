.weui-cell:nth-of-type(1)::before {
  border-top: 0;
}

.weui-cell__bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  font-size:30rpx;
}

.weui-cell__ft_in-access {
  position: relative;
}

.weui-cell__ft {
  text-align: right;
  color: rgba(0, 0, 0, 0.5);
}
.weui-cell__hd,.weui-cell__bd{
  vertical-align: middle;
  color: #666;
}
.cell-hd-image{
  margin-right: 30rpx;
  vertical-align: middle;
  width:44rpx; 
  height:44rpx;
}
.weui-cell__ft_in-access:after {
  content: " ";
  display: inline-block;
  height: 8px;
  width: 8px;
  border-width: 1px 1px 0 0;
  border-color: #aaa;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  position: relative;
  top: -2px;
  top: 50%;
  margin-top: -5px;
  right: 0;
}
