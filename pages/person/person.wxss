  height: 50rpx;
}

.persontxt .box-title {
  font-size: 30rpx;
  color: #666;
  flex-shrink: 100;
  flex: 2;
  padding: 30rpx 0;
  border-bottom: 1rpx solid #eee;
}

.persontxt .time:last-child .box-title {
  border-bottom: 0 none;
}

.personNav {
